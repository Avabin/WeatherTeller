<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:weatherForecast="clr-namespace:WeatherTeller.ViewModels.WeatherForecast;assembly=WeatherTeller.ViewModels"
             xmlns:weatherForecast1="clr-namespace:WeatherTeller.Avalonia.Views.WeatherForecast"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WeatherTeller.Avalonia.Views.WeatherForecast.WeatherForecastsView"
             x:DataType="weatherForecast:WeatherForecastsViewModel">
    <Grid>
        <weatherForecast1:CurrentWeatherForecastView ViewModel="{Binding CurrentWeather}" />
    </Grid>
</UserControl>
